<UserControl x:Class="WpfAppTest.View.Graphic.RectangleEx"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfAppTest.View.Graphic"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Rectangle Width="100" Height="50" 
                       Stroke="Blue" 
                       StrokeThickness="6"
                       Fill="#ffff00" HorizontalAlignment="Left" 
                       RadiusX="10" RadiusY="10"/>
            <!--RadiusX,Y 꼭지점 부분 둥그스럽게 (모서리가 둥근 사각형)-->
            <Ellipse Height="50" Width="100"
                     Stroke="Black" StrokeThickness="5"
                     Fill="#ff0000" HorizontalAlignment="Left"/>
        </StackPanel>
        
        
    </Grid>
</UserControl>
